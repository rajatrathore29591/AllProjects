    <script type="text/javascript">
        // Load the SDK Asynchronously
        (function (d) {

            var js;
            var id = 'facebook-jssdk';
            var ref = d.getElementsByTagName('script')[0];
            if (d.getElementById(id)) { return; }
            js = d.createElement('script');
            js.id = id;
            js.async = true;
            js.src = "../../Scripts/Facebook.js";
            ref.parentNode.insertBefore(js, ref);
        }(document)
            );

        // Init the SDK upon load
        window.fbAsyncInit = function () {
            FB.init({
                appId: '852870828112184', // App ID
                //appId: '1453912941591279',
                channelUrl: '//' + window.location.hostname + '/channel', // Path to your Channel File
                status: false, // check login status
                cookie: true, // enable cookies to allow the server to access the session
                xfbml: true  // parse XFBML
            });




            FB.Event.subscribe('auth.statusChange', function (response) {
                if (response.authResponse) {
                    //--------------------------------Section To Get Image and Convert Base64------------------------------------\\
                    FB.api("/me/picture?width=722&height=780", function (response) {
                        var imageUrl = response.data.url;
                        convertImgToBase64(imageUrl, function (base64Img) {

                            $("#imgbase64").val(base64Img);
                            //alert(base64Img);

                        });
                    });
                    //-----------------------------------------------------------------------------------------------------------\\

                    //---------------------------------Section To Get Full Profile------------------------------------------------\\
                    FB.api('/me', function (me) {
                        //console.log(me);
                        var imagebase64s = $('#imgbase64').val();

                        //-----------------------Facebook Call Ajax Section -----------------------------------------------------//
                        $.ajax({
                            url: '@Url.Action("Facebook", "Home")',
                            type: "POST",
                            data: { firstname: me.first_name, lastname: me.last_name, fbid: me.id, email: me.email, birthday: me.birthday, country: me.country, imagebase64: imagebase64s, gender: me.gender },
                            success: function (data) {

                                window.location.href = '../../Home/MyCourse';
                            },
                            error: function (result) {
                                $("#lblLoginerror").css("display", "block");
                            }
                        });
                        //--------------------Facebook Call Ajax Section Ends -----------------------------------------------------//
                    })
                    //------------------------------------------------------------------------------------------------------------\\


                    //-------------------------------Section to convert IMage Url to Base64----------------------------------------\\
                    function convertImgToBase64(url, callback, outputFormat) {
                        var canvas = document.createElement('CANVAS');
                        var ctx = canvas.getContext('2d');
                        var img = new Image;
                        img.crossOrigin = 'Anonymous';
                        img.onload = function () {
                            canvas.height = img.height;
                            canvas.width = img.width;
                            ctx.drawImage(img, 0, 0);
                            var dataURL = canvas.toDataURL(outputFormat || 'image/png');
                            callback.call(this, dataURL);
                            // Clean up
                            canvas = null;
                        };
                        img.src = url;
                    }
                    //-------------------------------------------------------------------------------------------------------------\\
                    document.getElementById('auth-loggedout').style.display = 'none';
                    document.getElementById('auth-loggedin').style.display = 'block';
                } else {
                    // user has not auth'd your app, or is not logged into Facebook
                    document.getElementById('auth-loggedout').style.display = 'block';
                    document.getElementById('auth-loggedin').style.display = 'none';
                }
            }, { scope: 'email,user_birthday,user_location,user_hometown' });
